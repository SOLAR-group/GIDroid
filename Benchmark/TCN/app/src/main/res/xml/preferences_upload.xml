<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="@string/preferences_category_settings_title">
        <SwitchPreferenceCompat
            app:defaultValue="@bool/preferences_upload_show_configurator_default_value"
            app:key="@string/preferences_upload_show_configurator_key"
            app:summaryOff="@string/preferences_upload_show_configurator_off_summary"
            app:summaryOn="@string/preferences_upload_show_configurator_on_summary"
            app:title="@string/preferences_upload_show_configurator_title" />
        <SwitchPreferenceCompat
            app:defaultValue="@bool/preferences_reupload_if_upload_fails_default_value"
            app:key="@string/preferences_reupload_if_upload_fails_key"
            app:summaryOff="@string/preferences_reupload_if_upload_fails_off_summary"
            app:summaryOn="@string/preferences_reupload_if_upload_fails_on_summary"
            app:title="@string/preferences_reupload_if_upload_fails_title" />
    </PreferenceCategory>
    <PreferenceCategory app:title="@string/ocid_name">
        <SwitchPreferenceCompat
            app:defaultValue="@bool/preferences_opencellid_enabled_default_value"
            app:key="@string/preferences_opencellid_enabled_key"
            app:summaryOff="@string/preferences_opencellid_enabled_off_summary"
            app:summaryOn="@string/preferences_opencellid_enabled_on_summary"
            app:title="@string/preferences_opencellid_enabled_title" />
        <SwitchPreferenceCompat
            app:defaultValue="@bool/preferences_opencellid_use_shared_api_key_default_value"
            app:key="@string/preferences_opencellid_use_shared_api_key_key"
            app:summaryOff="@string/preferences_opencellid_use_shared_api_key_off_summary"
            app:summaryOn="@string/preferences_opencellid_use_shared_api_key_on_summary"
            app:title="@string/preferences_opencellid_use_shared_api_key_title" />
        <info.zamojski.soft.towercollector.controls.TrimmedEditTextPreference
            app:defaultValue="@string/preferences_opencellid_api_key_default_value"
            app:key="@string/preferences_opencellid_api_key_key"
            app:summary="@string/preferences_opencellid_api_key_summary"
            app:title="@string/preferences_opencellid_api_key_title" />
        <PreferenceScreen
            app:key="@string/preferences_opencellid_api_key_link_key"
            app:summary="@string/preferences_opencellid_api_key_link_summary"
            app:title="@string/preferences_opencellid_api_key_link_title" />
    </PreferenceCategory>
    <PreferenceCategory app:title="@string/mls_name">
        <SwitchPreferenceCompat
            app:defaultValue="@bool/preferences_mls_enabled_default_value"
            app:key="@string/preferences_mls_enabled_key"
            app:summaryOff="@string/preferences_mls_enabled_off_summary"
            app:summaryOn="@string/preferences_mls_enabled_on_summary"
            app:title="@string/preferences_mls_enabled_title" />
    </PreferenceCategory>
    <PreferenceCategory app:title="@string/preferences_category_help_title">
        <PreferenceScreen
            app:key="@string/preferences_about_opencellid_api_key_key"
            app:summary="@string/preferences_about_opencellid_api_key_summary"
            app:title="@string/preferences_about_opencellid_api_key_title" />
        <PreferenceScreen
            app:key="@string/preferences_about_opencellid_project_key"
            app:summary="@string/preferences_about_opencellid_project_summary"
            app:title="@string/preferences_about_opencellid_project_title" />
        <PreferenceScreen
            app:key="@string/preferences_about_mls_project_key"
            app:summary="@string/preferences_about_mls_project_summary"
            app:title="@string/preferences_about_mls_project_title" />
    </PreferenceCategory>

</PreferenceScreen>