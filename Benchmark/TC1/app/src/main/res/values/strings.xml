    <string name="main_help_global_locations_title">Total number of measurements</string>
    <string name="main_help_global_locations_description">Number of measurements collected by the application since the beginning.</string>
    <string name="main_help_global_cells_title">Total number of discovered cells</string>
    <string name="main_help_global_cells_description">Number of newly discovered (unique) cells collected by the application since the beginning.</string>
    <string name="mail_help_last_number_of_cells_title">Number of cells</string>
    <string name="mail_help_last_number_of_cells_description">Number of main and neighboring cells to which the phone had access to.</string>
    <string name="main_help_last_network_type_title">Network type</string>
    <string name="main_help_last_network_type_description">The type of mobile network to which the phone was connected.</string>
    <string name="main_help_last_long_cell_id_title">Long Cell Tower ID</string>
    <string name="main_help_last_long_cell_id_description">The long unique cell tower identifier to which the phone was connected.\nFor UMTS networks the LCID, also called UTRAN CID, is a concatenation of RNC (ID of the Radio Network Controller) and Cell ID (unique ID of the Cell Tower).\nFor LTE networks the LCID, also called E-UTRAN CID, is a concatenation of eNodeB (ID of the Radio Network Controller) and Cell ID (unique ID of the Cell Tower).</string>
    <string name="main_help_last_cell_id_rnc_title">Cell Tower ID / RNC</string>
    <string name="main_help_last_cell_id_rnc_description">The unique cell tower identifier and RNC (ID of the Radio Network Controller) to which the phone was connected. This ID is defined only for UMTS and LTE networks.</string>
    <string name="main_help_last_cell_id_title">Cell Tower ID</string>
    <string name="main_help_last_cell_id_description">The unique cell tower identifier to which the phone was connected.</string>
    <string name="main_help_last_lac_title">Location Area Code</string>
    <string name="main_help_last_lac_description">The unique identifier assigned to group of cell towers to which the phone was connected.</string>
    <string name="main_help_last_mcc_title">Mobile Country Code</string>
    <string name="main_help_last_mcc_description">The unique country code of network operator to which the phone was connected.</string>
    <string name="main_help_last_mnc_title">Mobile Network Code</string>
    <string name="main_help_last_mnc_description">The unique identifier of network provider within country to which the phone was connected.</string>
    <string name="main_help_last_signal_strength_title">Signal strength</string>
    <string name="main_help_last_signal_strength_description">Network signal strength measured while saving expressed in dBm (power ratio in decibels (dB) of the measured power referenced to 1 milliwatt (mW)). Doubling this value means a 10 times stronger signal strength. Values ​​closer to 0 indicate stronger signal.</string>
    <string name="main_help_last_latitude_title">Latitude</string>
    <string name="main_help_last_latitude_description">The geographical latitude of saved position. Positive value ​​represents the northern hemisphere, negative southern. The higher is absolute value the more far it is away from the equator.</string>
    <string name="main_help_last_longitude_title">Longitude</string>
    <string name="main_help_last_longitude_description">The geographical longitude of saved position. Positive value represents a position east of the Prime Meridian, negative west. The higher is absolute value the more far it is away from the Prime Meridian.</string>
    <string name="main_help_last_gps_accuracy_title">GPS accuracy</string>
    <string name="main_help_last_gps_accuracy_description">Accuracy of the position obtained from GPS.</string>
    <string name="main_help_last_date_time_title">Save time</string>
    <string name="main_help_last_date_time_description">Date and time when last measurement was saved.</string>
